{
    "name": "Multiply",
    "comment": "Multiplies the left and right inputs of the same type together",
    "target": "Neutral",
    "inputs": [
        {
            "name": "left",
            "connectionType": "AutoDetect"
        },
        {
            "name": "right",
            "connectionType": "AutoDetect"
        }
    ],
    "outputs": [
        {
            "name": "output",
            "connectionType": "BasedOnInput",            
            "connectionTypeSource": "left"
        }
    ],
    "main": [
        "{output} = {left} * {right};"
    ]
}